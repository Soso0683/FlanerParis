            "liquid": "{% assign now_s = 'now' | date: '%s' %}\n{% assign tl3id = 'tl3-' | append: now_s %}\n\n<div id=\"{{ tl3id }}\" class=\"tl3de\"\n     data-ship-start=\"1\" data-ship-end=\"2\"\n     data-del-start=\"7\" data-del-end=\"9\">\n  <div class=\"tl3de__line\" aria-hidden=\"true\"></div>\n\n  <!-- √âtape 1 -->\n  <div class=\"tl3de__step\">\n    <div class=\"tl3de__dot\">\n      <img class=\"tl3de__icon\"\n           src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_054315_ca3eda7c-c44f-4188-a770-740919ab85d4.jpg?v=1754797492\"\n           alt=\"Warenkorb\">\n    </div>\n    <div class=\"tl3de__date\" data-role=\"order-date\">‚Äì</div>\n    <div class=\"tl3de__label\">Bestellt</div>\n  </div>\n\n  <!-- √âtape 2 -->\n  <div class=\"tl3de__step\">\n    <div class=\"tl3de__dot\">\n      <img class=\"tl3de__icon\"\n           src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_054424.jpg?v=1754797492\"\n           alt=\"Versand\">\n    </div>\n    <div class=\"tl3de__date\" data-role=\"ship-range\">‚Äì</div>\n    <div class=\"tl3de__label\">Versandt</div>\n  </div>\n\n  <!-- √âtape 3 -->\n  <div class=\"tl3de__step\">\n    <div class=\"tl3de__dot\">\n      <img class=\"tl3de__icon\"\n           src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_054435.jpg?v=1754797492\"\n           alt=\"Geliefert\">\n    </div>\n    <div class=\"tl3de__date\" data-role=\"deliv-range\">‚Äì</div>\n    <div class=\"tl3de__label\">Geliefert</div>\n  </div>\n</div>\n\n<style>\n  /* Conteneur : grille 3 colonnes = espacements √©gaux */\n  #{{ tl3id }}.tl3de{\n    --dot: 64px;      /* diam√®tre des ronds */\n    --icon: 28px;     /* taille des logos */\n    --line: 3px;      /* √©paisseur de la barre */\n    --fs-date: 13px;\n    --fs-label: 12px;\n    --black: #111;\n\n    color:#111; font-family:inherit;\n    display:grid; grid-template-columns:repeat(3, 1fr);\n    align-items:start; justify-items:center;\n    gap:0; position:relative;\n  }\n\n  /* Barre noire derri√®re les ronds */\n  #{{ tl3id }} .tl3de__line{\n    position:absolute;\n    left:calc(var(--dot)/2); right:calc(var(--dot)/2);\n    top:calc(var(--dot)/2 - var(--line)/2);\n    height:var(--line); background:var(--black);\n    z-index:0; /* derri√®re */\n  }\n\n  /* √âtapes */\n  #{{ tl3id }} .tl3de__step{ display:flex; flex-direction:column; align-items:center; text-align:center; }\n  #{{ tl3id }} .tl3de__dot{\n    width:var(--dot); height:var(--dot); border-radius:50%;\n    background:var(--black); display:grid; place-items:center; overflow:hidden;\n    position:relative; z-index:1; /* devant la barre */\n  }\n  #{{ tl3id }} .tl3de__icon{ width:var(--icon); height:var(--icon); object-fit:contain; display:block; }\n  #{{ tl3id }} .tl3de__date{ margin-top:10px; font-weight:700; font-size:var(--fs-date); }\n  #{{ tl3id }} .tl3de__label{ font-size:var(--fs-label); color:#5e5e5e; margin-top:2px; }\n\n  @media (max-width:600px){\n    #{{ tl3id }}.tl3de{ --dot:56px; --icon:24px; --fs-date:12px; --fs-label:11px; }\n  }\n</style>\n\n<script>\n(function(){\n  const root = document.getElementById('{{ tl3id }}'); if(!root) return;\n\n  const shipStart = +root.dataset.shipStart || 1;\n  const shipEnd   = +root.dataset.shipEnd   || 2;\n  const delStart  = +root.dataset.delStart  || 7;\n  const delEnd    = +root.dataset.delEnd    || 9;\n\n  const today = new Date(); // date du navigateur (visiteur)\n  const addDays = (d,n)=>{ const x = new Date(d); x.setDate(x.getDate()+n); return x; };\n\n  // Longs mois DE -> garde 3 ou 4 lettres (Jan, Feb, M√§rz, Apr, Mai, Juni, Juli, Aug, Sept, Okt, Nov, Dez)\n  const monthAbbrevLen = [3,3,4,3,3,4,4,3,4,3,3,3]; // index 0..11\n\n  function fmtAbbrev(date){\n    const d = new Date(date);\n    const day = new Intl.DateTimeFormat('de-DE', { day:'numeric' }).format(d);\n    const monthLong = new Intl.DateTimeFormat('de-DE', { month:'long' }).format(d);\n    const len = monthAbbrevLen[d.getMonth()];\n    const abbr = monthLong.substring(0, len);\n    return `${day} ${abbr}`;\n  }\n\n  root.querySelector('[data-role=\"order-date\"]').textContent =\n    fmtAbbrev(today);\n  root.querySelector('[data-role=\"ship-range\"]').textContent  =\n    `${fmtAbbrev(addDays(today, shipStart))} ‚Äì ${fmtAbbrev(addDays(today, shipEnd))}`;\n  root.querySelector('[data-role=\"deliv-range\"]').textContent =\n    `${fmtAbbrev(addDays(today, delStart))} ‚Äì ${fmtAbbrev(addDays(today, delEnd))}`;\n})();\n</script>"
