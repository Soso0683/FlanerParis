ve;\n  }\n  #{{ tl3id }} .tl3de__line{\n    position:absolute; left:calc(var(--dot)/2); right:calc(var(--dot)/2);\n    top:calc(var(--dot)/2 - var(--line)/2); height:var(--line); background:var(--black);\n  }\n  #{{ tl3id }} .tl3de__step{ display:flex; flex-direction:column; align-items:center; text-align:center; }\n  #{{ tl3id }} .tl3de__dot{\n    width:var(--dot); height:var(--dot); border-radius:50%; background:var(--black);\n    display:grid; place-items:center; overflow:hidden;\n  }\n  #{{ tl3id }} .tl3de__icon{\n    width:var(--icon); height:var(--icon); object-fit:contain; display:block;\n  }\n  #{{ tl3id }} .tl3de__date{ margin-top:10px; font-weight:700; font-size:var(--fs-date); }\n  #{{ tl3id }} .tl3de__label{ font-size:var(--fs-label); color:#5e5e5e; margin-top:2px; }\n\n  @media (max-width:600px){\n    #{{ tl3id }}.tl3de{ --dot:56px; --icon:24px; --fs-date:12px; --fs-label:11px; }\n  }\n</style>\n\n<script>\n(function(){\n  const root = document.getElementById('{{ tl3id }}'); if(!root) return;\n\n  const shipStart = +root.dataset.shipStart || 1;\n  const shipEnd   = +root.dataset.shipEnd   || 2;\n  const delStart  = +root.dataset.delStart  || 7;\n  const delEnd    = +root.dataset.delEnd    || 9;\n\n  // Date du système (visiteur)\n  const today = new Date();\n  const addDays = (d,n)=>{ const x=new Date(d); x.setDate(x.getDate()+n); return x; };\n\n  // Longs mois DE -> on ne garde que 3 ou 4 premières lettres selon le mois\n  const monthAbbrevLen = [3,3,4,3,3,4,4,3,4,3,3,3]; // Jan,Feb,März,Apr,Mai,Juni,Juli,Aug,Sept,Okt,Nov,Dez\n\n  function fmtAbbrev(date){\n    const d = new Date(date);\n    const day = new Intl.DateTimeFormat('de-DE', { day:'numeric' }).format(d); // \"10\"\n    const monthLong = new Intl.DateTimeFormat('de-DE', { month:'long' }).format(d); // ex \"September\"\n    const len = monthAbbrevLen[d.getMonth()];\n    const abbr = monthLong.substring(0, len); // \"Sept\", \"März\", \"Juni\", \"Juli\", etc.\n    return `${day} ${abbr}`;\n  }\n\n  root.querySelector('[data-role=\"order-date\"]').textContent = fmtAbbrev(today);\n  root.querySelector('[data-role=\"ship-range\"]').textContent  =\n    `${fmtAbbrev(addDays(today, shipStart))} – ${fmtAbbrev(addDays(today, shipEnd))}`;\n  root.querySelector('[data-role=\"deliv-range\"]').textContent =\n    `${fmtAbbrev(addDays(today, delStart))} – ${fmtAbbrev(addDays(today, delEnd))}`;\n})();\n</script>"
