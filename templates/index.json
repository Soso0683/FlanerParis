            "liquid": "{% assign now_s = 'now' | date: '%s' %}\n{% assign rid = 'reviews-' | append: now_s %}\n\n<div id=\"{{ rid }}\" class=\"rev-de\" aria-label=\"Kundenbewertungen\">\n  <div class=\"rev__viewport\" aria-live=\"polite\">\n    <div class=\"rev__track\">\n      <!-- Slide 1 -->\n      <article class=\"rev__slide\">\n        <header class=\"rev__head\">\n          <div class=\"rev__ava\">\n            <img class=\"rev__ava-img\" src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_125044.jpg?v=1754823152\" alt=\"Lisa K.\" loading=\"lazy\" onerror=\"this.style.display='none';this.nextElementSibling.style.display='grid'\">\n            <span class=\"rev__ava-fallback\">LK</span>\n          </div>\n          <p class=\"rev__text\">\n            Die Maske von <strong>Seoulume</strong> hat bei meinen Falten Wunder bewirkt.\n            Meine Haut wirkt <strong>fester</strong> und feine Linien sind <strong>gemildert</strong>.\n            Sie ist zu einem festen Teil meiner täglichen Routine geworden.\n          </p>\n        </header>\n        <footer class=\"rev__foot\">\n          <em class=\"rev__name\">Lisa K.</em>\n          <span class=\"rev__stars\" aria-label=\"5 von 5 Sternen\">\n            {% for i in (1..5) %}\n              <svg viewBox=\"0 0 20 20\" width=\"14\" height=\"14\" aria-hidden=\"true\"><path fill=\"#f5c048\" d=\"M10 1.6l2.4 4.9 5.3.8-3.9 3.8.9 5.4-4.7-2.5-4.7 2.5.9-5.4L2.3 7.3l5.3-.8L10 1.6z\"/></svg>\n            {% endfor %}\n          </span>\n        </footer>\n      </article>\n\n      <!-- Slide 2 -->\n      <article class=\"rev__slide\">\n        <header class=\"rev__head\">\n          <div class=\"rev__ava\">\n            <img class=\"rev__ava-img\" src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_125158.jpg?v=1754823152\" alt=\"Aurora J.\" loading=\"lazy\" onerror=\"this.style.display='none';this.nextElementSibling.style.display='grid'\">\n            <span class=\"rev__ava-fallback\">AJ</span>\n          </div>\n          <p class=\"rev__text\">\n            Meine Falten sind <strong>sichtbar zurückgegangen</strong> – schon nach wenigen Anwendungen.\n            <strong>Unglaublich!</strong>\n          </p>\n        </header>\n        <footer class=\"rev__foot\">\n          <em class=\"rev__name\">Aurora J.</em>\n          <span class=\"rev__stars\" aria-label=\"5 von 5 Sternen\">\n            {% for i in (1..5) %}\n              <svg viewBox=\"0 0 20 20\" width=\"14\" height=\"14\" aria-hidden=\"true\"><path fill=\"#f5c048\" d=\"M10 1.6l2.4 4.9 5.3.8-3.9 3.8.9 5.4-4.7-2.5-4.7 2.5.9-5.4L2.3 7.3l5.3-.8L10 1.6z\"/></svg>\n            {% endfor %}\n          </span>\n        </footer>\n      </article>\n\n      <!-- Slide 3 -->\n      <article class=\"rev__slide\">\n        <header class=\"rev__head\">\n          <div class=\"rev__ava\">\n            <img class=\"rev__ava-img\" src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_125136.jpg?v=1754823152\" alt=\"Melissa F.\" loading=\"lazy\" onerror=\"this.style.display='none';this.nextElementSibling.style.display='grid'\">\n            <span class=\"rev__ava-fallback\">MF</span>\n          </div>\n          <p class=\"rev__text\">\n            Meine Haut ist <strong>straffer</strong> und besser <strong>mit Feuchtigkeit versorgt</strong>.\n            Ich empfehle sie zu <strong>100&nbsp;%</strong>!\n          </p>\n        </header>\n        <footer class=\"rev__foot\">\n          <em class=\"rev__name\">Melissa F.</em>\n          <span class=\"rev__stars\" aria-label=\"5 von 5 Sternen\">\n            {% for i in (1..5) %}\n              <svg viewBox=\"0 0 20 20\" width=\"14\" height=\"14\" aria-hidden=\"true\"><path fill=\"#f5c048\" d=\"M10 1.6l2.4 4.9 5.3.8-3.9 3.8.9 5.4-4.7-2.5-4.7 2.5.9-5.4L2.3 7.3l5.3-.8L10 1.6z\"/></svg>\n            {% endfor %}\n          </span>\n        </footer>\n      </article>\n    </div>\n  </div>\n\n  <!-- Dots -->\n  <div class=\"rev__dots\" role=\"tablist\" aria-label=\"Navigation Bewertungen\">\n    <button type=\"button\" class=\"rev__dot is-active\" aria-label=\"Bewertung 1\" data-i=\"0\"></button>\n    <button type=\"button\" class=\"rev__dot\" aria-label=\"Bewertung 2\" data-i=\"1\"></button>\n    <button type=\"button\" class=\"rev__dot\" aria-label=\"Bewertung 3\" data-i=\"2\"></button>\n  </div>\n</div>\n\n<style>\n  #{{ rid }}.rev-de{font-family:inherit;color:#111;max-width:720px;margin:0 auto}\n  #{{ rid }} .rev__viewport{overflow:hidden}\n  #{{ rid }} .rev__track{display:flex;transition:transform .35s ease}\n  #{{ rid }} .rev__slide{min-width:100%;padding:14px 0}\n\n  #{{ rid }} .rev__head{display:flex;gap:12px;align-items:flex-start}\n  #{{ rid }} .rev__ava{width:44px;height:44px;border-radius:50%;overflow:hidden;flex:0 0 44px;display:grid;place-items:center;background:#eee}\n  #{{ rid }} .rev__ava-img{width:100%;height:100%;object-fit:cover;display:block}\n  #{{ rid }} .rev__ava-fallback{display:none;width:100%;height:100%;border-radius:50%;background:#ddd;color:#555;font-weight:700}\n  #{{ rid }} .rev__text{margin:0;font-size:15px;line-height:1.5}\n\n  #{{ rid }} .rev__foot{display:flex;align-items:center;gap:10px;margin-top:10px;border-top:1px solid #eee;padding-top:8px}\n  #{{ rid }} .rev__name{color:#8a8a8a}\n  #{{ rid }} .rev__stars{display:inline-flex;gap:2px;margin-left:auto}\n\n  #{{ rid }} .rev__dots{display:flex;justify-content:center;gap:10px;margin-top:8px}\n  #{{ rid }} .rev__dot{width:6px;height:6px;border-radius:50%;background:#bdbdbd;border:none;padding:0;cursor:pointer}\n  #{{ rid }} .rev__dot.is-active{width:8px;height:8px;background:#111}\n\n  @media (max-width:640px){\n    #{{ rid }} .rev__text{font-size:14px}\n  }\n</style>\n\n<script>\n(function(){\n  const root = document.getElementById('{{ rid }}'); if(!root) return;\n  const track = root.querySelector('.rev__track');\n  const slides = root.querySelectorAll('.rev__slide');\n  const dots = root.querySelectorAll('.rev__dot');\n  const count = slides.length;\n  let i = 0, w = root.querySelector('.rev__viewport').clientWidth;\n\n  function go(n, animate=true){\n    i = (n + count) % count;\n    track.style.transition = animate ? 'transform .35s ease' : 'none';\n    track.style.transform = `translateX(${-i * w}px)`;\n    dots.forEach((d,idx)=>d.classList.toggle('is-active', idx===i));\n  }\n\n  dots.forEach(d => d.addEventListener('click', () => go(+d.dataset.i)));\n\n  new ResizeObserver(()=>{ w = root.querySelector('.rev__viewport').clientWidth; go(i,false); }).observe(root);\n\n  // Swipe\n  let startX = 0, delta = 0, grabbing = false;\n  const vp = root.querySelector('.rev__viewport');\n  const start = x => { grabbing = true; startX = x; delta = 0; track.style.transition='none'; };\n  const move  = x => { if(!grabbing) return; delta = x - startX; track.style.transform = `translateX(${-(i*w) + delta}px)`; };\n  const end   = () => { if(!grabbing) return; grabbing=false; const thr = Math.max(40, w*0.12); if(delta > thr) go(i-1); else if(delta < -thr) go(i+1); else go(i); };\n\n  vp.addEventListener('pointerdown', e=>{vp.setPointerCapture(e.pointerId); start(e.clientX);});\n  vp.addEventListener('pointermove', e=>move(e.clientX));\n  vp.addEventListener('pointerup', end);\n  vp.addEventListener('pointercancel', end);\n  vp.addEventListener('touchstart', e=>start(e.touches[0].clientX), {passive:true});\n  vp.addEventListener('touchmove', e=>move(e.touches[0].clientX), {passive:true});\n  vp.addEventListener('touchend', end);\n\n  go(0,false);\n})();\n</script>"
